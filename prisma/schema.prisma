generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Survey {
  id        String   @id @default(cuid())
  rating    Int      // 1-5 stars
  feedback  String?  // optional text feedback
  createdAt DateTime @default(now())
  
  @@map("surveys")
}